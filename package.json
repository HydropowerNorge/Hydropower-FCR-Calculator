{
  "name": "fcr-calculator",
  "productName": "Hydropower",
  "version": "0.1.2",
  "description": "FCR-N Battery Income Calculator",
  "author": "DrivstoffAppen AS",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/HydropowerNorge/Hydropower-FCR-Calculator.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "convex:dev": "npx convex dev",
    "convex:dev:local": "CONVEX_AGENT_MODE=anonymous npx convex dev",
    "convex:deploy": "npx convex deploy",
    "convex:seed": "node scripts/seed-convex.js",
    "convex:seed:local": "CONVEX_AGENT_MODE=anonymous npx convex dev --once --run-sh \"node scripts/seed-convex.js\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-dmg": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron-forge/publisher-s3": "^7.11.1",
    "electron": "^32.2.0"
  },
  "dependencies": {
    "chart.js": "^4.4.7",
    "convex": "^1.31.7",
    "dotenv": "^17.3.1",
    "electron-squirrel-startup": "^1.0.1",
    "exceljs": "^4.4.0",
    "papaparse": "^5.4.1",
    "update-electron-app": "^3.1.2"
  }
}
