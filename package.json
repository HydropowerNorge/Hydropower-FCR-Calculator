    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "electron": "^32.2.0"
  },
  "dependencies": {
    "chart.js": "^4.4.7",
    "convex": "^1.31.7",
    "dotenv": "^17.3.1",
    "exceljs": "^4.4.0",
    "papaparse": "^5.4.1"
  }
}
